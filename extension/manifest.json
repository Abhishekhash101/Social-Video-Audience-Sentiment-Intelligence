{
    "manifest_version": 3,
    "name": "YouTube Sentiment Intelligence",
    "version": "1.0",
    "description": "Analyze audience sentiment on YouTube videos using ML.",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png", 
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
}